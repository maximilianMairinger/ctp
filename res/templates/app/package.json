{
  "scripts": {
    "pugToTypesService": "deno run --allow-write --allow-read mkComp.ts",
    "mkCompService": "deno run --allow-read --allow-env --allow-write pugToTypes.ts --watch",
    "pugToTypesOnce": "deno run --allow-read --allow-env --allow-write pugToTypes.ts",
    "iconBoilerplateHelperService": "deno run --allow-read --allow-write iconBoilerplateHelper.ts",
    "compressImages": "npx --yes image-web app/res/img public/res/img/dist --resolutions 4K,2K,PREV --algorithms jpg,webp,avif",
    "setEnvRepl": "npm run setEnv -- --preset repl",
    "setEnvProd": "npm run setEnv -- --preset prod",
    "setEnv": "node setEnv.js",
    "devApp": "    concurrently \"npm run buildDevReplServer\" \"node devServerInit.js --dev repl\"        \"npm run buildDevApp\"      \"npm run buildDevSw\"          \"npm run iconBoilerplateHelperService\" \"npm run pugToTypesService\" \"npm run mkCompService\" -n \"serv,node,_app,sewo,icon,pugo,comp\" -c \"bgBlue.bold,bgMagenta.bold,bgGreenBright.bold,bgCyan.bold,bgRed.bold,bgYellow.bold,bgRedBright.bold\"",
    "devServer": " concurrently \"npm run buildDevServer\"     \"node devServerInit.js --dev server\"      \"npm run buildDevReplApp\"                                                                                                                                   -n \"serv,node,_app\"                     -c \"bgBlue.bold,bgMagenta.bold,bgGreenBright.bold,bgCyan.bold\"",
    "dev": "       concurrently \"npm run buildDevServer\"     \"node devServerInit.js --dev server\"      \"npm run buildDevApp\"      \"npm run buildDevSw\"          \"npm run iconBoilerplateHelperService\" \"npm run pugToTypesService\" \"npm run mkCompService\" -n \"serv,node,_app,sewo,icon,pugo,comp\" -c \"bgBlue.bold,bgMagenta.bold,bgGreenBright.bold,bgCyan.bold,bgRed.bold,bgYellow.bold,bgRedBright.bold\"",
    "build": "     concurrently \"npm run buildProdServer\"    \"npm run buildProdApp\"                    \"npm run buildProdSw\"      \"npm run buildProdReplServer\"                                                                                                  -n \"serv,_app,sewo,rese\"                -c \"bgBlue.bold,bgMagenta.bold,bgGreenBright.bold,bgCyan.bold\"",
    "start": "wait-on server/dist && node ./server/dist/server.js",
    "startRepl": "wait-on replServer/dist && node ./replServer/dist/server.js",
    "buildProdSw": "rollup --config rollup.sw.prod.config.js",
    "buildProdServer": "rollup --config rollup.server.prod.config.js",
    "buildProdApp": "npm run pugToTypesOnce && webpack --config webpack.app.prod.config.js",
    "buildProdReplApp": "webpack --config webpack.replApp.prod.config.js",
    "buildProdReplServer": "rollup --config rollup.replServer.prod.config.js",
    "buildDevSw": "rollup --config rollup.sw.dev.config.js -w",
    "buildDevServer": "rollup --config rollup.server.dev.config.js -w",
    "buildDevApp": "npm run pugToTypesOnce && webpack --config webpack.app.dev.config.js",
    "buildDevReplApp": "webpack --config webpack.replApp.dev.config.js",
    "buildDevReplServer": "rollup --config rollup.replServer.dev.config.js -w"
  },

{
  "name": "${nameAsDashCase}",
  "version": "0.0.1",
  "description": "${description}",
  "main": "app/dist/${name}.js",
  "types": "app/dist/${name}.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.prod.json",
    "dev": "webpack --config webpack.dev.config.js",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/${githubUsername}/${name}.git"
  },
  "keywords": $<exec-code>return JSON.stringify(keywords, undefined, "  ")$</exec-code>,
  "author": "${author}",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/${githubUsername}/${name}/issues"
  },
  "homepage": "https://github.com/${githubUsername}/${name}#readme",
  "devDependencies": {
    "@types/node": "^12.6.9",
    "@types/tween.js": "^17.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "postcss-loader": "^3.0.0",
    "terser-webpack-plugin": "^1.2.2",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^5.3.3",
    "typescript": "^3.5.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-inject-plugin": "^1.5.2",
    "webpack-merge": "^4.2.2"
  }
}
